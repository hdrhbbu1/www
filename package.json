  "scripts": {
    "start": "gatsby develop",
    "lint": "eslint src/",
    "test": "jest",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "new": "node scripts/new.js",
    "check": "npm run lint && npm run test"
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/globals.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/**/__tests__/*.js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswyoung/web.git"
