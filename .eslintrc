{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": 1,
    "space-before-function-paren": 0,
    "react/prop-types": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "graphql": true
  }
}
