  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "react-helmet": "5.2.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
