    "shaka-player": "2.2.8",
    "styled-components": "2.2.4",
    "transliteration": "1.6.2",
    "typeface-gentium-book-basic": "0.0.44",
    "typeface-open-sans": "0.0.45",
    "typography": "0.16.6",
    "typography-plugin-code": "0.16.11"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--max-old-space-size=4096 gatsby develop",
    "lint": "eslint src/",
    "test": "jest",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 gatsby build",
    "serve": "gatsby serve",
    "new": "node scripts/new.js",
    "check": "npm run lint && npm run test"
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/globals.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/**/__tests__/*.js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswyoung/web.git"
  },
  "author": "Nicholas Young <nicholas@nicholaswyoung.com>",
  "license": "SEE LICENSE IN LICENSE.",
  "bugs": {
    "url": "https://github.com/nicholaswyoung/web/issues"
  },
