    "fs-extra": "4.0.3",
    "gatsby": "1.9.128",
    "gatsby-cli": "1.1.25",
    "gatsby-link": "1.6.30",
    "gatsby-plugin-catch-links": "1.0.13",
    "gatsby-plugin-feed": "1.3.14",
