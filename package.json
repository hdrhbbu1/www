    "gatsby-source-filesystem": "1.5.9",
    "gatsby-transformer-remark": "1.7.23",
    "gatsby-transformer-sharp": "1.6.15",
    "gatsby-transformer-yaml": "1.5.12",
    "howler": "2.0.5",
    "inquirer": "4.0.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "21.2.1",
    "js-yaml": "3.10.0",
    "left-pad": "1.2.0",
    "lodash.get": "4.4.2",
