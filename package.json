{
  "name": "nicholaswyoung.com",
  "version": "1.0.0",
  "description": "A Gatsby-powered blog and portfolio for me.",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "date-fns": "^1.28.4",
    "enzyme": "2.9.1",
    "es6-object-assign": "^1.1.0",
    "eslint": "4.6.1",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-standard": "3.0.1",
    "fs-extra": "4.0.1",
    "gatsby": "1.9.22",
    "gatsby-cli": "1.1.6",
    "gatsby-link": "1.6.16",
    "gatsby-plugin-catch-links": "1.0.8",
    "gatsby-plugin-feed": "1.3.9",
    "gatsby-plugin-glamor": "1.6.7",
    "gatsby-plugin-google-analytics": "1.0.7",
    "gatsby-plugin-manifest": "1.0.7",
    "gatsby-plugin-nprogress": "1.0.7",
    "gatsby-plugin-offline": "1.0.9",
    "gatsby-plugin-react-helmet": "1.0.6",
    "gatsby-plugin-sharp": "1.6.5",
    "gatsby-plugin-sitemap": "1.2.5",
    "gatsby-remark-copy-linked-files": "1.5.6",
    "gatsby-remark-images": "1.5.10",
    "gatsby-remark-prismjs": "1.2.7",
    "gatsby-remark-responsive-iframe": "1.4.7",
    "gatsby-remark-smartypants": "1.4.7",
    "gatsby-source-filesystem": "1.4.12",
    "gatsby-transformer-remark": "1.7.6",
    "gatsby-transformer-sharp": "1.6.5",
    "gatsby-transformer-yaml": "1.5.7",
    "greenkeeper-lockfile": "1.8.1",
    "howler": "^2.0.3",
    "inquirer": "3.2.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "20.0.4",
    "left-pad": "1.1.3",
    "lodash.get": "^4.4.2",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "15.6.1",
    "react-form": "^1.3.0",
    "react-router-scroll": "github:ytase/react-router-scroll#build",
    "react-test-renderer": "15.6.1",
    "react-typography": "0.16.5",
    "transliteration": "^1.6.2",
    "typeface-gentium-book-basic": "0.0.35",
    "typeface-open-sans": "0.0.35",
    "typography": "0.16.5",
    "typography-plugin-code": "^0.15.10"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--max-old-space-size=4096 gatsby develop",
    "lint": "eslint src/",
    "test": "jest",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 gatsby build",
    "serve": "gatsby serve",
    "new-post": "node -r babel-register scripts/new-post.js",
    "check": "npm run lint && npm run test"
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/globals.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/**/__tests__/*.js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswyoung/web.git"
  },
  "author": "Nicholas Young <nicholas@nicholaswyoung.com>",
  "license": "SEE LICENSE IN LICENSE.",
  "bugs": {
    "url": "https://github.com/nicholaswyoung/web/issues"
  },
  "homepage": "https://github.com/nicholaswyoung/web#readme"
}
