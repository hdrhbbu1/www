{
  "name": "nicholaswyoung.com",
  "version": "1.0.0",
  "description": "A Gatsby-powered blog and portfolio for me.",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "date-fns": "1.29.0",
    "enzyme": "3.1.0",
    "eslint": "4.10.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "3.0.1",
    "fs-extra": "4.0.2",
    "gatsby": "1.9.82",
    "gatsby-cli": "1.1.13",
    "gatsby-link": "1.6.23",
    "gatsby-plugin-catch-links": "1.0.9",
    "gatsby-plugin-feed": "1.3.11",
    "gatsby-plugin-glamor": "1.6.8",
    "gatsby-plugin-google-analytics": "1.0.10",
    "gatsby-plugin-manifest": "1.0.8",
    "gatsby-plugin-nprogress": "1.0.7",
    "gatsby-plugin-offline": "1.0.10",
    "gatsby-plugin-react-helmet": "1.0.8",
    "gatsby-plugin-react-next": "1.0.4",
    "gatsby-plugin-sharp": "1.6.13",
    "gatsby-plugin-sitemap": "1.2.5",
    "gatsby-plugin-styled-components": "1.0.5",
    "gatsby-remark-copy-linked-files": "1.5.14",
    "gatsby-remark-images": "1.5.24",
    "gatsby-remark-prismjs": "1.2.8",
    "gatsby-remark-responsive-iframe": "1.4.13",
    "gatsby-remark-smartypants": "1.4.8",
    "gatsby-source-filesystem": "1.5.5",
    "gatsby-transformer-remark": "1.7.18",
    "gatsby-transformer-sharp": "1.6.13",
    "gatsby-transformer-yaml": "1.5.11",
    "howler": "2.0.5",
    "inquirer": "3.3.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "21.2.1",
    "left-pad": "1.1.3",
    "lodash.get": "4.4.2",
    "normalize.css": "7.0.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-form": "2.1.5",
    "react-router-scroll": "github:ytase/react-router-scroll#build",
    "react-test-renderer": "16.0.0",
    "react-typography": "0.16.5",
    "shaka-player": "2.2.4",
    "styled-components": "2.2.3",
    "transliteration": "1.6.2",
    "typeface-gentium-book-basic": "0.0.43",
    "typeface-open-sans": "0.0.43",
    "typography": "0.16.6",
    "typography-plugin-code": "0.15.11"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--max-old-space-size=4096 gatsby develop",
    "lint": "eslint src/",
    "test": "jest",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 gatsby build",
    "serve": "gatsby serve",
    "new-post": "node -r babel-register scripts/new-post.js",
    "check": "npm run lint && npm run test"
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/globals.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/**/__tests__/*.js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswyoung/web.git"
  },
  "author": "Nicholas Young <nicholas@nicholaswyoung.com>",
  "license": "SEE LICENSE IN LICENSE.",
  "bugs": {
    "url": "https://github.com/nicholaswyoung/web/issues"
  },
  "homepage": "https://github.com/nicholaswyoung/web#readme"
}
